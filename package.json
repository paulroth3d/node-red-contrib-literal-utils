{
  "name": "node-red-contrib-literal-utils",
  "version": "1.0.1",
  "description": "Node Red Utility Nodes to work with literals and arrays.",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha --config .mocharc.json \"test/**/*_spec.js\" --exit",
    "test:watch": "npm run test -- --watch",
    "lint": "node node_modules/eslint-watch/bin/esw -c .eslintrc.json nodes test",
    "lint:watch": "node node_modules/eslint-watch/bin/esw -c .eslintrc.json nodes test -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulroth3d/node-red-contrib-literal-utils.git"
  },
  "node-red": {
    "nodes": {
      "pick-array-value": "nodes/array/pick-array-value.js"
    }
  },
  "keywords": [
    "node-red", "literals", "array", "utility"
  ],
  "author": "Paul Roth <proth@salesforce.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulroth3d/node-red-contrib-literal-utils/issues"
  },
  "homepage": "https://github.com/paulroth3d/node-red-contrib-literal-utils#readme",
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/node-red": "^0.20.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-watch": "^5.1.2",
    "mocha": "6.1.4",
    "node-red": "0.20.5",
    "node-red-node-test-helper": "0.2.2",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "fancy-log": "^1.3.3"
  }
}
